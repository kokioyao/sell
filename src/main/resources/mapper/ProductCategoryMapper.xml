<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.footman.sell.dataobject.mapper.ProductCategoryMapper">
    <select id="selectByCategoryType" parameterType="Integer" resultType="cn.footman.sell.dataobject.ProductCategory">
        select * from product_category where category_type = #{categoryType}
    </select>
</mapper>